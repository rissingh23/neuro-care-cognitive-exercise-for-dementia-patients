{
  "expo": {
    "name": "Neuro-Care - Cognitive Exercise for Dementia Patients",
    "slug": "neuro-care-cognitive-exercise-for-dementia-patients",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "helloworld",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rissinghorg.neurocarecognitiveexercisefordementiapatients"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rissinghorg.neurocarecognitiveexercisefordementiapatients"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bf3cbe82-9742-454b-8cb7-09d19df15a30"
      }
    },
    "owner": "rissingh-org",
    "updates": {
      "url": "https://u.expo.dev/bf3cbe82-9742-454b-8cb7-09d19df15a30"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
